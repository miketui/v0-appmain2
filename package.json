    "build:staging": "NODE_ENV=staging VITE_APP_ENV=staging vite build",
    "build:production": "NODE_ENV=production VITE_APP_ENV=production vite build",
    "start": "next start",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
