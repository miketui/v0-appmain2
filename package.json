    "deploy:staging:local": "./scripts/deploy-staging.sh local",
    "staging:check": "npm run test && npm run lint && npm run type-check && npm audit --audit-level=high",
    "staging:build": "npm run staging:check && npm run build:staging",
    "staging:deploy": "npm run staging:build && npm run deploy:staging:railway",
    "health:staging": "curl -f https://hausofbasquiat-staging.railway.app/health",
    "logs:staging": "railway logs --service staging",
    "lighthouse:staging": "lighthouse-ci --config=.lighthouserc.json"
